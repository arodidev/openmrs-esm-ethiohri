      },
      {
        "id": "mobiletelephoneNumber",
        "label": "Mobile Telephone Number",
        "type": "person attribute",
        "uuid": "1794bcad-b2d2-47ae-8e26-1d4ec31ade13",
        "validation": {
          "matches": "^[0-9]{10}$"
        }
      },
      {
        "id": "obs-test-phoneNumber",
        "label": "Obs-Test Phone number",
        "type": "obs",
        "uuid": "159635AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"
      },
      {
        "id": "obs-test-fullName",
        "label": "Obs-Test Full name",
        "type": "obs",
        "uuid": "166103AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"
      },
      {
        "id": "obs-test-registrationDate",
        "label": "Obs-Test Registration date",
        "type": "obs",
        "uuid": "160753AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"
      },
      {
        "id": "obs-test-gender",
        "label": "Obs-Test Gender",
        "type": "obs",
        "uuid": "1533AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
